{
  "name": "app-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/main/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsc-paths -p tsconfig.build.json -s ./src -o ./dist",
    "start": "npm run build && node dist/main/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "prettier-config-standard": "^5.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsc-paths-resolver": "^0.1.3",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
